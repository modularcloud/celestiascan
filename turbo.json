{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "web#build": {
      "env": [
        "WHITELABEL",
        "CELESTIA_MOCHA_RPC",
        "DYMENSION_HUB_RPC",
        "DYMENSION_ROLLAPP_X_RPC",
        "ADD_NETWORK_PASS",
        "ADD_NETWORK_ENDPOINT",
        "SOLANA_RPC"
      ]
    }
  }
}
